{
  "name": "foobar",
  "version": "0.1.0",
  "description": "foobar",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && node dist/src/populate-orm-cache.js && scripts/copy-graphql-files",
    "format": "prettier \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:fix": "yarn format --write",
    "format:check": "yarn format --check",
    "generate": "ts-node generate-typings",
    "generate:graphql-types": "ts-node generate-typings",
    "generate:merged-sdl": "ts-node src/write-merged-sdl",
    "generate:env": "./scripts/one_pass_to_env.sh",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:repl": "nest start --entryFile repl",
    "start:konsumer": "nest start -c nest-cli.konsumer.json",
    "start:konsumer:dev": "nest start -c nest-cli.konsumer.json --watch",
    "start:konsumer:debug": "nest start -c nest-cli.konsumer.json --debug --watch",
    "start:konsumer:prod": "node dist/src/main-konsumer",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers 3 --config ./jest-unit.config.js",
    "clean:db-test": "NODE_ENV=test NODE_PATH=. mikro-orm schema:fresh -r",
    "test:watch": "jest --watch --config ./jest-unit.config.js",
    "test:cov": "jest --maxWorkers 3 --coverage --config ./jest-unit.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest-unit.config.js",
    "test:e2e": "yarn clean:db-test && jest --config ./jest-e2e.config.js",
    "migrate": "mikro-orm migration:up && pigeon migration:up"
  },
  "dependencies": {
    "@apollo/subgraph": "^0.4.2",
    "@fastify/static": "^6.0.0",
    "@mikro-orm/core": "^5.6.0",
    "@mikro-orm/nestjs": "^5.1.2",
    "@mikro-orm/postgresql": "^5.6.0",
    "@mikro-orm/reflection": "^5.6.0",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/mercurius": "^10.1.7",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-fastify": "^9.2.1",
    "@nestjs/terminus": "^9.1.1",
    "@sentry/hub": "^6.19.7",
    "@sentry/node": "^6.19.7",
    "@trialspark/pigeon.ts": "^1.2.0",
    "@trialspark/toolbox": "^0.4.2",
    "dd-trace": "^3.8.0",
    "graphql": "^16.4.0",
    "lodash": "^4.17.21",
    "mercurius": "^10.0.0",
    "nest-winston": "^1.7.0",
    "nestjs-ddtrace": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.1.0",
    "@mikro-orm/cli": "^5.6.0",
    "@mikro-orm/migrations": "^5.6.0",
    "@mikro-orm/seeder": "^5.6.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@trialspark/pigeon.cli": "^1.1.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.40",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.0.0",
    "jest-mock-extended": "^2.0.7",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.72.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./dist/mikro-orm.config.js"
    ]
  }
}
